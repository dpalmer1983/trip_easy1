<div class="page-header">
  <h3>Date Preference #<%= @date_preference.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @date_preference.user_id %></dd>

      <dt>Date options</dt>
      <dd><%= @date_preference.date_options_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/date_preferences" class="btn btn-primary">
        Back
      </a>
      <a href="/date_preferences/<%= @date_preference.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_date_preference/<%= @date_preference.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
