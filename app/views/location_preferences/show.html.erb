<div class="page-header">
  <h3>Location Preference #<%= @location_preference.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @location_preference.user_id %></dd>

      <dt>Location options</dt>
      <dd><%= @location_preference.location_options_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/location_preferences" class="btn btn-primary">
        Back
      </a>
      <a href="/location_preferences/<%= @location_preference.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_location_preference/<%= @location_preference.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
